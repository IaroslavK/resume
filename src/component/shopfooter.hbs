<div class="col-12">
    <div class="row">
        {{#each footer}}
        <div class="col-2">
            <div class="nav flex-column">
                <div class="nav-item">
                    {{#each this}}

                    <a href={{link}} {{#if @last}} class="nav-link disabled" {{else}} class="nav-link" {{/if}}>
                        {{text}}</a>

                    {{/each}}
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>